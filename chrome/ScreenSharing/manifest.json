{
  "name" : "Mentor Meetings - Screen Sharing Plugin",
  "author": "Custom Solutions Group, LLC",
  "version" : "1.1",
  "manifest_version" : 2,
  "minimum_chrome_version": "34",
  "description" : "This Chrome extension is used for screen sharing during a Mentor Meeting",
  "homepage_url": "https://meeting.mentorhq.com",
  "background": {
    "scripts": ["background-script.js"],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://meeting.mentorhq.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
